<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent">


    <TextView
        android:id="@+id/tvlabelname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="105dp"
        android:text="Name : "
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvlabelusn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="154dp"
        android:text="USN : "
        android:textSize="15sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tvlabelsemester"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvlabelname"
        android:layout_alignParentTop="true"
        android:layout_marginTop="203dp"
        android:textStyle="bold"
        android:textSize="15sp"
        android:text="Semester : " />

    <TextView
        android:id="@+id/tvlabelbranch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvlabelname"
        android:layout_alignParentTop="true"
        android:layout_marginTop="253dp"
        android:textStyle="bold"
        android:textSize="15sp"
        android:text="Branch : " />


    <TextView
        android:id="@+id/tvlabelsection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tvlabelname"
        android:layout_alignParentTop="true"
        android:layout_marginTop="303dp"
        android:textStyle="bold"
        android:textSize="15sp"
        android:text="Section : " />

    <TextView
        android:id="@+id/etnamestudent"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="61dp"
        android:layout_toEndOf="@+id/tvlabelname"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/etusnstudent"
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="154dp"
        android:layout_marginEnd="107dp"
        android:layout_toEndOf="@+id/tvlabelusn"
        android:ems="10"
        android:hint="USN"
        android:inputType="textPersonName"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/etsemstudent"
        android:layout_width="231dp"
        android:layout_height="37dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="204dp"
        android:layout_marginEnd="56dp"
        android:layout_toEndOf="@+id/tvlabelsemester"
        android:ems="10"
        android:hint="Semester"
        android:inputType="textPersonName"
        android:scrollbars="horizontal"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/etbranchstudent"
        android:layout_width="242dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="62dp"
        android:layout_toEndOf="@+id/tvlabelbranch"
        android:ems="10"
        android:hint="Branch"
        android:inputType="textPersonName"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/etsectionstudent"
        android:layout_width="190dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="299dp"
        android:layout_marginEnd="105dp"
        android:layout_toEndOf="@+id/tvlabelsection"
        android:ems="10"
        android:hint="Section"
        android:inputType="textPersonName"
        android:textSize="15sp" />


</RelativeLayout>